    threads: THREADS
    shell: """
           bowtie2 -p {threads} {BOWTIE_HISAT_NOFW_NORC} -a -x {WORKING_FOLDER}/standard_results/{WORKFLOW_NAME}/Index/{ASSEMBLY_VERSION} -U {input.fastq} -S {output}
